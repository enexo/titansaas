<h1>Kroger Dashboard</h1>
<script>

    jcontent.(document).ready(function () {
        var html = '<table class="table table-striped">';
        html += '<tr>';
        var flag = 0;
        $.each(data[0], function(index, value){
            html += '<th>'+index+'</th>';
        });
        html += '</tr>';
        $.each(data, function(index, value){
            html += '<tr>';
            $.each(value, function(index2, value2){
                html += '<td>'+value2+'</td>';
            });
            html += '<tr>';
        });
        html += '</table>';
        $('body').html(html);
    });









</script>
